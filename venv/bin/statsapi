#!/Users/Sam/Desktop/repos/statsapi/cli_tool/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'statsapi','console_scripts','statsapi'
__requires__ = 'statsapi'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('statsapi', 'console_scripts', 'statsapi')()
    )
